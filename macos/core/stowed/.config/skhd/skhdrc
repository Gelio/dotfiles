fn - h : yabaictl focus-window west
fn - j : yabaictl focus-window south
fn - k : yabaictl focus-window north
fn - l : yabaictl focus-window east

fn + shift - h : yabai -m window --warp west
fn + shift - j : yabai -m window --warp south
fn + shift - k : yabai -m window --warp north
fn + shift - l : yabai -m window --warp east

fn + ctrl - h : yabaictl move-space west
fn + ctrl - j : yabaictl move-space south
fn + ctrl - k : yabaictl move-space north
fn + ctrl - l : yabaictl move-space east

fn - n : yabaictl focus-space next
fn - p : yabaictl focus-space previous

fn - 1 : yabaictl focus-space --stable-index 1 --create-if-not-found
fn - 2 : yabaictl focus-space --stable-index 2 --create-if-not-found
fn - 3 : yabaictl focus-space --stable-index 3 --create-if-not-found
fn - 4 : yabaictl focus-space --stable-index 4 --create-if-not-found
fn - 5 : yabaictl focus-space --stable-index 5 --create-if-not-found
fn - 6 : yabaictl focus-space --stable-index 6 --create-if-not-found
fn - 7 : yabaictl focus-space --stable-index 7 --create-if-not-found
fn - 8 : yabaictl focus-space --stable-index 8 --create-if-not-found
fn - 9 : yabaictl focus-space --stable-index 9 --create-if-not-found
fn - 0 : yabaictl focus-space --stable-index 10 --create-if-not-found

fn + shift - 1 : yabaictl move-window 1 --create-if-not-found
fn + shift - 2 : yabaictl move-window 2 --create-if-not-found
fn + shift - 3 : yabaictl move-window 3 --create-if-not-found
fn + shift - 4 : yabaictl move-window 4 --create-if-not-found
fn + shift - 5 : yabaictl move-window 5 --create-if-not-found
fn + shift - 6 : yabaictl move-window 6 --create-if-not-found
fn + shift - 7 : yabaictl move-window 7 --create-if-not-found
fn + shift - 8 : yabaictl move-window 8 --create-if-not-found
fn + shift - 9 : yabaictl move-window 9 --create-if-not-found
fn + shift - 0 : yabaictl move-window 10 --create-if-not-found

fn + shift - space : yabai -m window --toggle float

# Stacking support
fn - w : yabai -m space --layout stack
fn - e : yabai -m space --layout bsp
fn + ctrl - n : yabai -m window --focus stack.next || yabai -m window --focus stack.first
fn + ctrl - p : yabai -m window --focus stack.prev || yabai -m window --focus stack.last

# vim: ft=ini
