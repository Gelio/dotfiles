fn - h : yabaictl focus-window west
fn - j : yabaictl focus-window south
fn - k : yabaictl focus-window north
fn - l : yabaictl focus-window east

fn + shift - h : yabai -m window --warp west
fn + shift - j : yabai -m window --warp south
fn + shift - k : yabai -m window --warp north
fn + shift - l : yabai -m window --warp east

fn + ctrl - h : yabaictl move-space west
fn + ctrl - j : yabaictl move-space south
fn + ctrl - k : yabaictl move-space north
fn + ctrl - l : yabaictl move-space east

fn - n : yabaictl focus-space next
fn - p : yabaictl focus-space previous

fn - 1 : yabaictl focus-space --stable-index 1
fn - 2 : yabaictl focus-space --stable-index 2
fn - 3 : yabaictl focus-space --stable-index 3
fn - 4 : yabaictl focus-space --stable-index 4
fn - 5 : yabaictl focus-space --stable-index 5
fn - 6 : yabaictl focus-space --stable-index 6
fn - 7 : yabaictl focus-space --stable-index 7
fn - 8 : yabaictl focus-space --stable-index 8
fn - 9 : yabaictl focus-space --stable-index 9
fn - 0 : yabaictl focus-space --stable-index 10

fn + shift - 1 : yabai -m window --space 1
fn + shift - 2 : yabai -m window --space 2
fn + shift - 3 : yabai -m window --space 3
fn + shift - 4 : yabai -m window --space 4
fn + shift - 5 : yabai -m window --space 5
fn + shift - 6 : yabai -m window --space 6
fn + shift - 7 : yabai -m window --space 7
fn + shift - 8 : yabai -m window --space 8
fn + shift - 9 : yabai -m window --space 9

fn + shift - space : yabai -m window --toggle float

# Stacking support
fn - w : yabai -m space --layout stack
fn - e : yabai -m space --layout bsp
fn + ctrl - n : yabai -m window --focus stack.next || yabai -m window --focus stack.first
fn + ctrl - p : yabai -m window --focus stack.prev || yabai -m window --focus stack.last

# vim: ft=ini
