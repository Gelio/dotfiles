lalt - h : yabaictl focus-window west
lalt - j : yabaictl focus-window south
lalt - k : yabaictl focus-window north
lalt - l : yabaictl focus-window east

lalt + shift - h : yabaictl move-window in-direction west
lalt + shift - j : yabaictl move-window in-direction south
lalt + shift - k : yabaictl move-window in-direction north
lalt + shift - l : yabaictl move-window in-direction east

lalt + ctrl - h : yabaictl move-space west --create-extra-space-if-last-on-display
lalt + ctrl - j : yabaictl move-space south --create-extra-space-if-last-on-display
lalt + ctrl - k : yabaictl move-space north --create-extra-space-if-last-on-display
lalt + ctrl - l : yabaictl move-space east --create-extra-space-if-last-on-display

lalt - n : yabaictl focus-space next --destroy-empty-background-spaces
lalt - p : yabaictl focus-space previous --destroy-empty-background-spaces

lalt - 1 : yabaictl focus-space --stable-index 1 --create-if-not-found --destroy-empty-background-spaces
lalt - 2 : yabaictl focus-space --stable-index 2 --create-if-not-found --destroy-empty-background-spaces
lalt - 3 : yabaictl focus-space --stable-index 3 --create-if-not-found --destroy-empty-background-spaces
lalt - 4 : yabaictl focus-space --stable-index 4 --create-if-not-found --destroy-empty-background-spaces
lalt - 5 : yabaictl focus-space --stable-index 5 --create-if-not-found --destroy-empty-background-spaces
lalt - 6 : yabaictl focus-space --stable-index 6 --create-if-not-found --destroy-empty-background-spaces
lalt - 7 : yabaictl focus-space --stable-index 7 --create-if-not-found --destroy-empty-background-spaces
lalt - 8 : yabaictl focus-space --stable-index 8 --create-if-not-found --destroy-empty-background-spaces
lalt - 9 : yabaictl focus-space --stable-index 9 --create-if-not-found --destroy-empty-background-spaces
lalt - 0 : yabaictl focus-space --stable-index 10 --create-if-not-found --destroy-empty-background-spaces

lalt + shift - 1 : yabaictl move-window to-space 1 --create-if-not-found
lalt + shift - 2 : yabaictl move-window to-space 2 --create-if-not-found
lalt + shift - 3 : yabaictl move-window to-space 3 --create-if-not-found
lalt + shift - 4 : yabaictl move-window to-space 4 --create-if-not-found
lalt + shift - 5 : yabaictl move-window to-space 5 --create-if-not-found
lalt + shift - 6 : yabaictl move-window to-space 6 --create-if-not-found
lalt + shift - 7 : yabaictl move-window to-space 7 --create-if-not-found
lalt + shift - 8 : yabaictl move-window to-space 8 --create-if-not-found
lalt + shift - 9 : yabaictl move-window to-space 9 --create-if-not-found
lalt + shift - 0 : yabaictl move-window to-space 10 --create-if-not-found

lalt + shift - space : yabai -m window --toggle float

# Manage splits
lalt + shift - t : yabai -m window --toggle split
lalt - b : yabai -m space --balance

# Stacking support
lalt - w : yabai -m space --layout stack
lalt - e : yabai -m space --layout bsp
lalt + ctrl - n : yabai -m window --focus stack.next || yabai -m window --focus stack.first
lalt + ctrl - p : yabai -m window --focus stack.prev || yabai -m window --focus stack.last

# Kitty quake-style terminal

lalt - t : kitten quick-access-terminal

